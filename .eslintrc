{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6
	},
	"rules": {
		"comma-dangle": [2,"always-multiline"],
		"no-cond-assign": [2,"always"],
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": [2,"all"],
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 1,
		"valid-typeof": 2,
		"no-unexpected-multiline": 2,
		"curly": [2,"all"],
		"default-case": 2,
		"dot-location": [2,"property"],
		"eqeqeq": [2,"smart"],
		"guard-for-in": 2,
		"no-caller": 2,
		"no-else-return": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-invalid-this": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-octal": 2,
		"no-redeclare": [2,{"builtinGlobals":false}],
		"no-useless-call": 2,
		"yoda": [2,"never",{"onlyEquality":true}],
		"no-catch-shadow": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-undefined": 2,
		"no-unused-vars": 1,
		"no-use-before-define": [2,"nofunc"],
		"brace-style": [2,"1tbs",{"allowSingleLine":true}],
		"comma-style": 2,
		"eol-last": 2,
		"indent": ["error","tab"],
		"linebreak-style": [2,"unix"],
		"new-parens": 2,
		"newline-after-var": [2,"always"],
		"no-lonely-if": 2,
		"no-spaced-func": 2,
		"no-unneeded-ternary": 2,
		"quote-props": ["error","as-needed"],
		"quotes": [2,"single","avoid-escape"],
		"semi": [2,"always"],
		"sort-vars": 2,
		"space-before-function-paren": ["error","never"],
		"space-in-parens": [2,"always", {"exceptions": ["empty"]}],
		"space-infix-ops": 2,
		"space-return-throw-case": 2,
		"space-unary-ops": [2,{"words":true}],
		"spaced-comment": [2,"always",{}],
		"arrow-parens": [2,"as-needed"],
		"arrow-spacing": [2,{"before":true,"after":true}],
		"constructor-super": 2,
		"no-const-assign": 2,
		"no-this-before-super": 2,
		"no-var": 2,
		"object-shorthand": [1,"always"],
		"prefer-const": 2,
		"prefer-spread": 2,
		"prefer-reflect": 2,
		"require-yield": 2
	}
}
